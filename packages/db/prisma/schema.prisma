generator client {
  provider = "prisma-client-js"
  output   = "./src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(nanoid(7))
  username  String   @unique
  password  String
  avatar    String?
  createdAt DateTime @default(now())
}
